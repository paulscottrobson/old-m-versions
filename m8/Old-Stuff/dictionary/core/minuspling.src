; ****************************************************************
; ****************************************************************
;
;;      Name  -!
;;      Type  word
;
;;$		Subtract B from 16 bit word at A
;
; ****************************************************************
; ****************************************************************

		ld 		a,(hl)
		sub 	e
		ld 		(hl),a
		inc 	hl
		ld 		a,(hl)
		sbc 	a,d
		ld 		(hl),a
		ret