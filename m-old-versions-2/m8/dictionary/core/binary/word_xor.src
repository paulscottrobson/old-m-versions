; *******************************************************************************
; *******************************************************************************
;
;      @name      xor
;      @type      word
;      @desc      Xor B into A, B unchanged.
;
; *******************************************************************************
; *******************************************************************************

		ld 		a,h
		xor 	d
		ld 		h,a
		ld 		a,l
		xor 	e
		ld 		l,a
		ret
